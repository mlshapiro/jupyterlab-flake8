{
  "name": "jupyterlab-flake8",
  "version": "0.6.1",
  "description": "jupyterlab extension to lint python code in the notebook and text editor",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/mlshapiro/jupyterlab-flake8",
  "bugs": {
    "url": "https://github.com/mlshapiro/jupyterlab-flake8/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Marc Shapiro",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "schema/*.json",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/mlshapiro/jupyterlab-flake8.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "shx rm -rf lib",
    "watch": "tsc -w",
    "prettier": "prettier --write '**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}'"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.0.2",
    "@jupyterlab/apputils": "^2.0.2",
    "@jupyterlab/cells": "^2.0.2",
    "@jupyterlab/codemirror": "^2.0.2",
    "@jupyterlab/coreutils": "^4.0.2",
    "@jupyterlab/fileeditor": "^2.0.2",
    "@jupyterlab/mainmenu": "^2.0.2",
    "@jupyterlab/notebook": "^2.0.2",
    "@jupyterlab/settingregistry": "^2.0.1",
    "@jupyterlab/statedb": "^2.0.1",
    "@jupyterlab/terminal": "^2.0.2",
    "@types/codemirror": "0.0.76",
    "codemirror": "~5.58.2"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^2.0.2",
    "shx": "^0.3.2",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "~3.7.4",
    "minimist": "^1.2.3"
  },
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema"
  }
}
